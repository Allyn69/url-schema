    <script type="text/javascript">

        // Fake MsComCustomEvent function to avoid JS error on page because WEDCS has been removed
        window.MscomCustomEvent = function () { };

        $(window).load(function () {
            $.cachedScript("https://answersstaticfilecdn.azureedge.net/static/js/mwf/mwf-main.var.1.53.1.min.js").done(function () {
                $(document).trigger("mwfloaded");
            });

            if (Answers.CookieCompliance.IsCookieConsentRequired) {
                try {
                    var scriptfiles = [];
                    var chained = $.cachedScript(scriptfiles[0]);
                    for (var i = 1; i < scriptfiles.length; i++) {
                        chained = chained.pipe($.cachedScript(scriptfiles[i]));
                    }

                    chained.done(function (data) {
                        if (typeof (mscc) !== "undefined") {
                            mscc.on("consent", function () { Answers.CookieCompliance.IsCookieConsentRequired = false; });
                        }
                    });
                } catch (e) {
                    console.warn(e);
                }
            }
        });
    </script>

