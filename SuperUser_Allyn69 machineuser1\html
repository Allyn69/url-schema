        var privateMessagesLabel = 'View private messages';
        var privateMessagesUrl = 'https://answers.microsoft.com/en-us/privatemessage/inbox?id=00000000-0000-0000-0000-000000000000';
        var viewProfileUrl = 'https://answers.microsoft.com/en-us/profile/00000000-0000-0000-0000-000000000000';

        var meControlInitOptions = {
            containerId: 'meControl',
            enabled: true,
            headerHeight: 48,

            mobileBreakpoints: {
                shortHeader: 1083,
                mobile: 767
            },
            rpData:
             {
                 msaInfo:
                 {
                     signInUrl: signInUrl,
                     signOutUrl: signOutUrl,
                 },
                 aadInfo:
                 {
                     appId: meControlInfo.data("app-id"),
                     signInUrl: signInUrl,
                     signOutUrl: signOutUrl,
                 },
                 preferredIdp: 'msa',
             },

            signInStr: signInLabel,
            signOutStr: signOutLabel,
            market: $("html").attr("lang")
        };

        var shellOptions = {
            meControlOptions: {
                rpData: {
                    msaInfo: {
                        signInUrl: signInUrl,
                        signOutUrl: signOutUrl,
                    },
                    aadInfo:
                    {
                        appId: meControlInfo.data("app-id"),
                        signInUrl: signInUrl,
                        signOutUrl: signOutUrl,
                    },
                    preferredIdp: "msa"
                },
                userData: {
                    idp: meControlInfo.data("auth-type"),
                    firstName: "",
                    lastName: "",
                    memberName: "",
                    cid: "",
                    authenticatedState: meControlInfo.attr("data-auth-state")
                },
                events:{
                    onSignIn: function() {
                        location.href = signInUrl;
                    },
                    onBeforeSignOut: function() {
                        location.href = signOutUrl;
                    }
                }
            }
        };

        var loadShellWithUserData = function() {
            shellOptions.meControlOptions.userData.authenticatedState = meControlInfo.attr("data-auth-state");
            shellOptions.meControlOptions.userData.cid =  meControlInfo.data("cid");
            shellOptions.meControlOptions.userData.tileUrl = meControlInfo.data("profile-image-url");
            shellOptions.meControlOptions.extensibleLinks = [
                                                                    {
                                                                        string: viewProfileLabel,
                                                                        url: viewProfileUrl
                },
                {
                    string: privateMessagesLabel,
                    url: privateMessagesUrl
                }
            ];

            var email = meControlInfo.data("email");
            var displayName = meControlInfo.data("display-name");
            if (email.length > 0) {
                shellOptions.meControlOptions.userData.memberName = email;
                shellOptions.meControlOptions.userData.nickName = displayName;
            }
            else {
                shellOptions.meControlOptions.userData.memberName = displayName;
            }

            window.msCommonShell.load(shellOptions);
        };

        var loadShell = function() {
            if (window.msCommonShell) {
                if (meControlInfo.data("is-authenticated") == true) {
                    loadShellWithUserData();
                } else {
                    window.msCommonShell.load(shellOptions);
                }
            }
        };

        if (window.msCommonShell) {
            loadShell();
        } else {
            window.onShellReadyToLoad = function () {
                window.onShellReadyToLoad = null;
                loadShell();
            }
        }
    </script>

